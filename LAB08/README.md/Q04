#include<stdio.h>
#include<string.h>
int main()
{
	char password[8];
	char previous_password[]="secure12";
	printf("Enter your password");
	scanf("%s",&password[8]);
	if(strlen(password[8])<8)
	{
		printf("Password must be of 8 characters");
	}
	else
	{
		if(strcmp(password[8],previous_password)==0)
		{
			printf("Login successful, welcome");
		}
		else
		{
			printf("Login failed, try again");
		}
	}
	return 0;
}
