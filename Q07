#include <stdio.h>
int main() 
{
    int n;
    printf("Enter the number of elements: ");
    scanf("%d", &n);
    int arr[n],sum = 0;
    printf("Enter the elements of the array:\n");
    for (int i = 0; i < n; i++) 
	{
        scanf("%d", &arr[i]);
        sum=sum+arr[i];
    }
    printf("The sum is: %d\n", sum);
    return 0;
}
